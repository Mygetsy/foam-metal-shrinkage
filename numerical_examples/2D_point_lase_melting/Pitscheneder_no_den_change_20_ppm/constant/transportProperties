FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    //transportModel  Newtonian;
    transportModel  Newtonian;
    nu              53.13e-07;   // Kim: nu = 8.56e-07, mu = 6.42e-3
    rho             8100;       // Kim: at T = 1700

    quasiIncompressible true;

    rhoSolid        8100;
    dRhoDT.solid    (0 0);
    dRhoDT.liquid   (0 0);
    massCorrectionCoeff 0.5;
}

gas
{
    transportModel  Newtonian;

    // Argon: nu = 2.76e-4, mu = 7.9e-5
    // Air: 1.516e-5 for 293K, 4.270e-4 for 2000K
    nu              2.76e-4;

    // Argon: 0.29
    // Air: 1.204 for 293 K, 0.1553 for 2000K
    rho             2.9;
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial
    (
        (-31.19         0)
        (0.07691        1)
        (-7.397e-05     2)
        (3.785e-08      3)
        (-1.092e-11     4)
        (1.684e-15      5)
        (-1.083e-19     6)
    );
}

beta            beta            [ K^-1 ] 0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ] 1e+9; // 1e+12 for PDAS=750nm
qMushyCoeff     qMushyCoeff                  1e-15; // 1e+12 for PDAS=750nm

