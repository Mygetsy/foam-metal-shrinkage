FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    transportModel  Newtonian;
    nu              3.0e-7;   // Kim: nu = 8.56e-07, mu = 6.42e-3
    rho             6093;       // Kim: at T = 1700

    quasiIncompressible true;

    rhoSolid        6093;
    dRhoDT.solid    (0 0);
    dRhoDT.liquid   (0 0);
    tauCorr         1e-1;
}

gas
{
    transportModel  Newtonian;

    nu              1.e4;
    rho             0.4;
}

sigma
{
    type            constant;
    sigma           0.0;
}

surfaceForces
{
    sigma
    {
        type            constant;
        sigma           0.0;
    }
  surfaceTensionForceModel gradAlpha;
  accelerationForceModel gravity;
  deltaFunctionModel alphaCSF;
}

beta            beta            [ K^-1 ] 0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ] 5e+9; // 1e+12 for PDAS=750nm
qMushyCoeff     qMushyCoeff                  1e-15; // 1e+12 for PDAS=750nm

